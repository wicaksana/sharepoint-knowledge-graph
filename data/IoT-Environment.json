        "columns": [
          {
            "id": "1",
            "webparts@odata.context": "https://graph.microsoft.com/v1.0/$metadata#sites('d4782aca-aacc-4f4f-a6d1-af9e0f41c4be')/pages('f6a2be01-0fd8-4937-aa78-0df82539e0af')/microsoft.graph.sitePage/canvasLayout/horizontalSections('2')/columns('1')/webparts",
            "webparts": [
              {
                "id": "95c052f1-d429-43e3-9e14-20d8cea540c2",
                "@odata.type": "#microsoft.graph.textWebPart",
                "innerHtml": "<p class=\"noSpacingAbove noSpacingBelow\" data-text-type=\"noSpacing\">Telkomsel IoT's solution in energy management provides real-time monitoring and measurement of energy consumption, including electricity and other forms of energy, using sensors and MCUs (Master Control Unit) that transmit data to a web dashboard.</p><p class=\"noSpacingAbove noSpacingBelow\" aria-hidden=\"true\" data-text-type=\"noSpacing\">&nbsp;</p><p class=\"noSpacingAbove noSpacingBelow\" data-text-type=\"noSpacing\"><strong>Why IoT Envion is the right solution for your business</strong></p><p class=\"noSpacingAbove noSpacingBelow\" aria-hidden=\"true\" data-text-type=\"noSpacing\">&nbsp;</p><ul class=\"customListStyle\"><li>Optimizing Energy Performance - Energy usage becomes efficient with data, reports, and predictive maintenance.</li><li>Business Insight - Making business decisions based on insights from reports and data analysis.</li><li>Provides Visibility of Energy Usage - Monitoring energy via features that provide visual energy consumption, alarms, and status.</li><li>Business Operation Efficiency - Remote energy monitoring through field equipment access and alarm settings.</li></ul>"
              },
              {
                "id": "4ab659f7-6aad-4bbe-91ad-e6c70e76cb4e",
                "@odata.type": "#microsoft.graph.textWebPart",
                "innerHtml": "<p class=\"noSpacingAbove noSpacingBelow\" data-text-type=\"noSpacing\"><strong>IoT Environment Features</strong></p><p class=\"noSpacingAbove noSpacingBelow\" data-text-type=\"noSpacing\">&nbsp;</p><p class=\"noSpacingAbove noSpacingBelow\" data-text-type=\"noSpacing\">Energy usage monitoring to mitigate risks through an integrated system for real-time power monitoring and continuous active management.</p><p class=\"noSpacingAbove noSpacingBelow\" aria-hidden=\"true\" data-text-type=\"noSpacing\">&nbsp;</p>"
              }
            ],
            "width": 12
          }
        ],
        "emphasis": "none",
        "layout": "oneColumn"
      },

            "webparts@odata.context": "https://graph.microsoft.com/v1.0/$metadata#sites('d4782aca-aacc-4f4f-a6d1-af9e0f41c4be')/pages('fc8157b4-cb7f-482e-8afb-800688fd8ee8')/microsoft.graph.sitePage/canvasLayout/horizontalSections('2.5')/columns('1')/webparts",
            "webparts": [
              {
                "id": "eadb28a0-61a4-4d03-986f-0c0d3de52ff5",
                "@odata.type": "#microsoft.graph.textWebPart",
                "innerHtml": "<p class=\"noSpacingAbove noSpacingBelow\" style=\"margin-left:0px;\" data-text-type=\"noSpacing\"><span class=\"fontSizeXxLarge\"><strong>Overview</strong></span></p><p class=\"noSpacingAbove spacingBelow\" style=\"margin-left:0px;\" aria-hidden=\"true\" data-text-type=\"withSpacing\">&nbsp;</p><p class=\"noSpacingAbove spacingBelow\" style=\"margin-left:0px;\" data-text-type=\"withSpacing\">At our company, we specialize in <strong>IoT solutions for smart manufacturing and industrial applications</strong>, transforming traditional industries into digitally driven, data-powered operations. Our solutions leverage advanced IoT technologies to optimize manufacturing processes, enhance operational efficiency, and provide real-time insights that enable data-driven decision-making.</p>"
              },
              {
                "id": "73bb5841-199b-4f22-9142-7389df68c8dc",
                "@odata.type": "#microsoft.graph.standardWebPart",
                "data": {
                  "dataVersion": "1.12",
                  "description": "Add an image, picture or photo to your page including text overlays and ability to crop and resize images.",
                  "serverProcessedContent": {
                    "htmlStrings": [],
                    "imageSources": [
                      {
                        "key": "imageSource",
                        "value": "https://cdn.hubblecontent.osi.office.net/m365content/publish/984eb06b-e13d-4c1c-a05c-4f1f490993f1/1025633076.jpg"
                      }
                    ],
                    "links": [],
                    "searchablePlainTexts": [
                      {
                        "key": "captionText",
                        "value": "Using drone to remotely monitor the crops"
                      }
                    ]
                  },
                  "title": "Image",
                  "properties": {
                    "imageSourceType": 2,
                    "altText": "",
                    "linkUrl": "",
                    "overlayText": "Using drone to remotely monitor the crops",
                    "fileName": "Drone-flying-over-a-field.jpg",
                    "imgWidth": 4866,
                    "imgHeight": 3243,
                    "minimumLayoutWidth": 1,
